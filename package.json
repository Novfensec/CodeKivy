{
  "name": "codekivy",
  "main": "./src/extension.js",
  "publisher": "Kartavya Shukla",
  "displayName": "CodeKivy",
  "description": "Modern Kivy and KvLang support for VSCode ",
  "version": "0.0.1",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Novfensec/CodeKivy"
  },
  "license": "MIT",
  "keywords": [
    "kivy",
    "kvlang",
    "syntax",
    "highlighting",
    "language",
    "extension",
    "vscode",
    "codekivy",
    "kv",
    "python",
    "programming",
    "development",
    "editor"
  ],
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "kvlang",
        "aliases": [
          "KvLang",
          "kvlang",
          "kv"
        ],
        "extensions": [
          ".kv"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "kvlang",
        "path": "./snippets/kvlang.json"
      }
    ],
    "grammars": [
      {
        "language": "kvlang",
        "scopeName": "source.kvlang",
        "path": "./syntaxes/kvlang.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "ignore": "^7.0.5"
  }
}